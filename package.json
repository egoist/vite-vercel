{
  "private": true,
  "name": "vite-vercel-workspace",
  "scripts": {
    "build": "turbo run build",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "bumpp": "^8.2.1",
    "turbo": "^1.4.4"
  },
  "version": "0.1.15",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
